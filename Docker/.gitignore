# Ignore all environment and secret files
.env
*.env
!env.example

# Ignore all secrets, keys, and credentials
secrets/
*.key
*.pem
*.crt
*.cert
*.p12
*.jks
*.asc
*.gpg
*.enc

config/backend/uploads/**
!config/backend/uploads/.keep

# Ignore database and runtime data
volumes/
data/
logs/

# Ignore backup and temp files
*.bak
*.tmp
*.swp

# Ignore Docker build cache
**/.cache/
**/cache/

# Ignore system files
.DS_Store
Thumbs.db

# Ignore other sensitive files
*.secret
*.password

# Allow docker-compose.yml and README.md
!docker-compose.yml
!README.md
